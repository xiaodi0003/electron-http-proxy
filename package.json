{
  "name": "electron-http-proxy",
  "version": "0.2.0",
  "private": true,
  "author": "xiaodi0003",
  "description": "HTTP proxyer and capturer",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue src-vue server",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue src-vue server",
    "prettier": "prettier -c --write \"**/*\"",
    "start-electron": "export PORT=8001 && electron . --inspect=5858",
    "dev": "concurrently \"yarn dev:vue\" \"yarn start-electron\"",
    "package": "electron-builder build --mac --publish never",
    "build-package": "yarn build:vue && yarn package",
    "update-package:asar": "yarn build:vue && node scripts/update-asar.js",
    "anyproxy-ca": "anyproxy-ca",
    "dev:vue": "vite --config vite.config.ts",
    "build:vue": "vite build --config vite.config.ts",
    "preview:vue": "vite preview --config vite.config.ts"
  },

  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "anyproxy": "^4.1.3",
    "electron-store": "^5.2.0",
    "element-plus": "^2.9.0",
    "https-proxy-agent": "^5.0.0",
    "mime": "^2.4.6",
    "pinia": "^2.1.0",
    "socks": "^2.8.6",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@rollup/rollup-darwin-x64": "^4.52.3",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/tsconfig": "^0.5.0",
    "concurrently": "^5.3.0",
    "electron": "^9.1.0",
    "electron-builder": "^26.0.12",
    "eslint": "^7.1.0",
    "prettier": "^2.0.1",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "productName": "ElectronHttpProxy",
  "main": "server/electron.js",
  "build": {
    "files": [
      "build/**/*",
      "server/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "electron-http-proxy",
      "owner": "xiaodi0003"
    }
  }
}
